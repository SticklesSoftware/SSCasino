@using SSCasino;
@using SSCasino.Models;
@model YouTubeAPI

@Html.DevExpress().CallbackPanel(
    settings =>
    {
        // Assign general and shared properties
        DevExpressHelpers.Render_CallbackPanel(settings, "cbp_Thumbnails", "Home", "APIDemos_RefreshThumbnails", "", true, true);

        // Content
        settings.SetContent(() =>
        {
            Html.RenderPartial("_APIDemos_Thumbnails", Model);
        });
    }
).GetHtml()

